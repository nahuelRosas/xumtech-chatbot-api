{
  "name": "xumtech-chatbot",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.*\" --fix",
    "format": "prettier --write \"**/**/*.{js,jsx,ts,tsx,json,md,html,css}\" --cache",
    "check:update": "ncu --interactive --format group",
    "generate:env": "if grep -q 'NODE_ENV' .env.development.local; then echo ENV64_CONFIG=$(openssl base64 -A -in .env.development.local) > .env && grep 'NODE_ENV' .env.development.local >> .env; else echo ENV64_CONFIG=$(openssl base64 -A -in .env.development.local) > .env && echo NODE_ENV=development >> .env; fi && if grep -q 'ORIGIN_URL' .env.development.local; then grep 'ORIGIN_URL' .env.development.local >> .env; else echo ORIGIN_URL=http://localhost:3000 >> .env; fi",
    "depcheck": "depcheck --ignore-dirs=dist,build,coverage,node_modules,.next,.nuxt,.serverless,.vercel,.output,.cache --ignore-patterns=**/*.d.ts,**/*.spec.ts,**/*.test.ts,**/*.test.js,**/*.e2e-spec.ts,**/*.e2e-spec.js,**/jest.config.js,**/jest-preset.js,**/tsconfig.json,**/tsconfig.*.json --ignores=multer,@types/multer,prettier-plugin-ini,prettier-plugin-properties"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/openai": "^2.0.19",
    "@google-cloud/storage": "^7.17.0",
    "@mastra/core": "^0.14.1",
    "@mastra/libsql": "^0.13.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@types/busboy": "^1.5.4",
    "busboy": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "eslint-plugin-nestjs": "^1.2.3",
    "express": "^5.1.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0",
    "mastra": "^0.10.23",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "string-similarity-js": "^2.1.4",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@nestjs/cli": "^11.0.10",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "depcheck": "^1.4.7",
    "eslint": ">=9.34.0",
    "globals": "^16.3.0",
    "npm-check-updates": "^18.0.2",
    "prettier": "^3.6.2",
    "prettier-plugin-ini": "^1.3.0",
    "prettier-plugin-properties": "^0.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  },
  "packageManager": "pnpm@10.15.0"
}
